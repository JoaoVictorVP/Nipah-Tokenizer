namespace NipahTokenizer.NanoDesu
{
    /// <summary>
    /// A nano expression is a expression that expresses a token in unary format (for expression processing)
    /// </summary>
    public struct NanoExpression : IMiiExpression
    {
        public ExpressionType type => ExpressionType.Unary;

        public Token token;

        public NanoExpression(Token token)
        {
            this.token = token;
        }

        public void Run()
        {
            throw new System.NotImplementedException();
        }
    }
}
